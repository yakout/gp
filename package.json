{
  "name": "highlight-detector",
  "version": "1.0.0",
  "description": "A system that detects highlights of sports videos",
  "main": "backend/server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "server-install": "npm install --prefix backend",
    "dev-install": "npm install && npm install --prefix client && npm install --prefix backend",
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix client",
    "test-server": "cd backend && npm run test",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yakout/gp.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yakout/gp/issues"
  },
  "homepage": "https://github.com/yakout/gp#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.11"
  },
  "dependencies": {
    "react-bootstrap-table-next": "^3.1.5"
  }
}
